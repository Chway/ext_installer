{
	"name": "ExtInstaller",
	"description": "Install and update extensions from everywhere.",
	"version": "1.0.0",
	"key": "",
	"manifest_version": 3,
	"background": {
		"service_worker": "js/init.js"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"permissions": ["management", "storage", "contextMenus", "alarms", "downloads"],
	"host_permissions": ["<all_urls>"]
}
